{
    "env": {
        "browser": false,
        "node": false,
        "amd": false
    },
    "rules": {
        "taboo-name": [1, "taboo"],


        "no-alert": 0,
        "no-caller": 0,
        "no-bitwise": 0,
        "no-catch-shadow": 0,
        "no-console": 0,
        "no-comma-dangle": 0,
        "no-control-regex": 0,
        "no-debugger": 0,
        "no-div-regex": 0,
        "no-dupe-keys": 0,
        "no-else-return": 0,
        "no-empty": 0,
        "no-empty-class": 0,
        "no-eq-null": 0,
        "no-eval": 0,
        "no-ex-assign": 0,
        "no-func-assign": 0,
        "no-floating-decimal": 0,
        "no-implied-eval": 0,
        "no-with": 0,
        "no-fallthrough": 0,
        "no-global-strict": 0,
        "no-unreachable": 0,
        "no-undef": 0,
        "no-undef-init": 0,
        "no-unused-expressions": 0,
        "no-octal": 0,
        "no-octal-escape": 0,
        "no-obj-calls": 0,
        "no-multi-str": 0,
        "no-new-wrappers": 0,
        "no-new": 0,
        "no-new-func": 0,
        "no-native-reassign": 0,
        "no-plusplus": 0,
        "no-delete-var": 0,
        "no-return-assign": 0,
        "no-new-array": 0,
        "no-new-object": 0,
        "no-label-var": 0,
        "no-ternary": 0,
        "no-self-compare": 0,
        "no-sync": 0,
        "no-underscore-dangle": 0,
        "no-loop-func": 0,
        "no-empty-label": 0,
        "no-unused-vars": 0,
        "no-script-url": 0,
        "no-proto": 0,
        "no-iterator": 0,
        "no-mixed-requires": [0, false],
        "no-wrap-func": 0,
        "no-shadow": 0,
        "no-use-before-define": 0,
        "no-redeclare": 0,

        "brace-style": 0,
        "block-scoped-var": 0,
        "camelcase": 0,
        "complexity": [0, 11],
        "consistent-this": [0, "that"],
        "curly": 0,
        "dot-notation": 0,
        "eqeqeq": 0,
        "guard-for-in": 0,
        "max-depth": [0, 4],
        "max-len": [0, 80, 4],
        "max-params": [0, 3],
        "max-statements": [0, 10],
        "new-cap": 0,
        "new-parens": 0,
        "one-var": 0,
        "quotes": [0, "double"],
        "quote-props": 0,
        "radix": 0,
        "regex-spaces": 0,
        "semi": 0,
        "smarter-eqeqeq": 0,
        "strict": 0,
        "unnecessary-strict": 0,
        "use-isnan": 0,
        "wrap-iife": 0,
        "wrap-regex": 0
    }
}
